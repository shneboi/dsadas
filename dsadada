-- Spawner Module
local Spawner = loadstring(game:HttpGet("https://codeberg.org/GrowAFilipino/GrowAGarden/raw/branch/main/Spawner.lua"))()

-- Spawn Button Functionality
SpawnBtn.MouseButton1Click:Connect(function()
	local name = PetName.Text
	local age = tonumber(Age.Text)
	local weight = tonumber(Weight.Text)

	if name ~= "" and age and weight then
		Spawner.SpawnPet(name, weight, age)
	else
		warn("‚ùå Invalid input. Make sure all fields are filled correctly.")
	end
end)
