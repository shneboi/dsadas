SpawnBtn.Size = UDim2.new(0, 299, 0, 36)
SpawnBtn.Font = Enum.Font.FredokaOne
SpawnBtn.Text = "SPAWN PET"
SpawnBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
SpawnBtn.TextSize = 20.000
SpawnBtn.MouseButton1Click:Connect(function()
	local petName = PetName.Text
	local weight = tonumber(Weight.Text)
	local age = tonumber(Age.Text)

	if petName == "" or not weight or not age then
		warn("‚ö†Ô∏è Please fill in all fields with valid values.")
		return
	end

	-- Spawn the pet using the Spawner module
	Spawner.SpawnPet(petName, weight, age)
end)
