-- Scripts

-- Minimize Button
MinimizeButton.MouseButton1Click:Connect(function()
    MinimizeButton.Visible = false
    Frame.Visible = true
end)

-- Exit Button
ExitBtn.MouseButton1Click:Connect(function()
    Frame.Visible = false
    MinimizeButton.Visible = true
end)

-- Rainbow Titles
local function RainbowText(textLabel)
    local hue = 0
    while true do
        hue = (hue + 0.01) % 1
        textLabel.TextColor3 = Color3.fromHSV(hue, 1, 1)
        task.wait(0.05)
    end
end
task.spawn(function() RainbowText(Title) end)
task.spawn(function() RainbowText(Title2) end)
task.spawn(function() RainbowText(Title3) end)

-- Freeze Trade & Auto Accept buttons
local isFreezeOn = false
local isAutoOn = false
FreezeTradeBtn.BackgroundColor3 = Color3.fromRGB(188, 0, 0)
AutoAcceptBtn.BackgroundColor3 = Color3.fromRGB(188, 0, 0)
FreezeTradeBtn.BorderColor3 = Color3.fromRGB(136, 37, 37)
AutoAcceptBtn.BorderColor3 = Color3.fromRGB(136, 37, 37)

FreezeTradeBtn.MouseButton1Click:Connect(function()
    isFreezeOn = not isFreezeOn
    if isFreezeOn then
        FreezeTradeBtn.Text = "FREEZE TRADE (ON)"
        FreezeTradeBtn.BackgroundColor3 = Color3.fromRGB(74, 167, 28)
        FreezeTradeBtn.BorderColor3 = Color3.fromRGB(113, 255, 42)
    else
        FreezeTradeBtn.Text = "FREEZE TRADE (OFF)"
        FreezeTradeBtn.BackgroundColor3 = Color3.fromRGB(188, 0, 0)
        FreezeTradeBtn.BorderColor3 = Color3.fromRGB(136, 37, 37)
    end
end)

AutoAcceptBtn.MouseButton1Click:Connect(function()
    isAutoOn = not isAutoOn
    if isAutoOn then
        AutoAcceptBtn.Text = "AUTO ACCEPT (ON)"
        AutoAcceptBtn.BackgroundColor3 = Color3.fromRGB(74, 167, 28)
        AutoAcceptBtn.BorderColor3 = Color3.fromRGB(113, 255, 42)
    else
        AutoAcceptBtn.Text = "AUTO ACCEPT (OFF)"
        AutoAcceptBtn.BackgroundColor3 = Color3.fromRGB(188, 0, 0)
        AutoAcceptBtn.BorderColor3 = Color3.fromRGB(136, 37, 37)
    end
end)

-- Spawn Pet Button
SpawnBtn.MouseButton1Click:Connect(function()
    local petName = PetName.Text
    local weight = tonumber(Weight.Text) or 0
    local age = tonumber(Age.Text) or 0

    if petName ~= "" then
        Spawner.SpawnPet(petName, weight, age)
        print("‚úÖ Spawned pet:", petName, weight .. "kg", "Age " .. age)
    else
        warn("‚ö† Please enter a pet name before spawning.")
    end
end)
